<?xml version="1.0" encoding="iso-8859-1" ?>
<!DOCTYPE parameters PUBLIC "presets" "presets.dtd" >
<presets>

  <preset param="pcw_wdt_peripheral_enable" name="0" strength="7">
      <set param="pcw_wdt_peripheral_freqmhz" value="133.333333"/>
      <disable param="pcw_wdt_peripheral_freqmhz"/>
      <disable param="pcw_wdt_wdt_io"/>
  </preset>

  <preset param="pcw_wdt_peripheral_enable" name="" strength="7">
      <set param="pcw_wdt_peripheral_freqmhz" value="133.333333"/>
      <disable param="pcw_wdt_peripheral_freqmhz"/>
      <disable param="pcw_wdt_wdt_io"/>
  </preset>

  <preset param="pcw_wdt_peripheral_enable" name="1" strength="4">
      <set  param="pcw_wdt_wdt_io" value="EMIO" />
